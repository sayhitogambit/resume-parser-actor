{
  "title": "Resume Parser Input",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "resumeText": {"title": "Resume Text", "type": "string", "editor": "textarea", "minLength": 50},
    "extractSkills": {"title": "Extract Skills", "type": "boolean", "default": true},
    "extractCertifications": {"title": "Extract Certifications", "type": "boolean", "default": true},
    "model": {"title": "AI Model", "type": "string", "enum": ["openai/gpt-4o", "anthropic/claude-3.5-sonnet"], "default": "openai/gpt-4o"},
    "openrouterApiKey": {"title": "OpenRouter API Key", "type": "string", "isSecret": true}
  },
  "required": ["resumeText", "openrouterApiKey"]
}
